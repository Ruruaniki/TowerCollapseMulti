.endgame-container {
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin-top: 50px;
}

.endgame-highscore {
    font-size: 50px;
    color: yellow;
    -unity-font-style: bold;
}

.endgame-score {
    font-size: 50px;
    color: white;
}

.endgame-retry {
    font-size: 36px;
    color: white;
}
